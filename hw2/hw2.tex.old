	\section{Problem 1.}
	\begin{description}
		\item \textit{Apply de Moivre's theorem to $(\cos\theta + i\sin\theta)^3$ to derive trigonometric identities for $\cos(3\theta)$ and $\sin(3\theta).$ \footnote{De Moivre's theorem: Let $w = r(\cos\alpha + i\sin\alpha) = re^{i\alpha}.$ Then $w^n = r^n(\cos(n\alpha) + i\sin(n\alpha)) = r^n e^{in\alpha}.$ }}
	\end{description}

	By expanding the polyomial we get
	\begin{align*}
		(\cos\theta + i\sin\theta)^3 &= \cos^3\theta - 3\cos\theta\sin^2\theta + i(3\cos^2\theta\sin\theta - \sin^3\theta),
	\end{align*}
	and by de Moivre's theorem
	\begin{align*}
		(\cos\theta + i\sin\theta)^3 &= \cos(3\theta) + i\sin(3\theta).
	\end{align*}

	Then $\cos(3\theta) + i\sin(3\theta) = \cos^3\theta - 3\cos\theta\sin^2\theta + i(3\cos^2\theta\sin\theta - \sin^3\theta),$ and equating their real and imaginary parts leaves us the following identities:
	\begin{align*}
		\cos(3\theta) &= \cos^3\theta - 3\cos\theta\sin^2\theta,\\
		\sin(3\theta) &= 3\cos^2\theta\sin\theta - \sin^3\theta.
	\end{align*}

	\section{Problem 2.}
	\begin{description}
		\item \textit{Show that if $\omega$ is any $n$-th root of unity not equal to $1$, then $1+\omega + \omega^2 + \cdots + \omega^{n-1} = 0.$ Hint: An $n$-th root of unity $z$ satisfies $z^n - 1 = 0.$ Now, factor out $z-1.$}
	\end{description}

	Let $\omega$ be an $n$-th root of unity not equal to $1$. Then $\omega^n -1 = 0.$ Then
	\[
		\omega^n - 1 = (\omega - 1)(1+\omega + \omega^2 + \cdots + \omega^{n-1}).
	\]

