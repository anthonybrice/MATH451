	\section{Problem 4.}
	\begin{description}
		\item \textit{Find all cube roots of $27i$, and all fourth roots of $-16$.}
	\end{description}

	\begin{align*}
		\sqrt[3]{27i} &= \sqrt[3]{27e^{i(\pi / 2 + 2\pi k)}} \; \forall \; k \in \Z.\\
		&= 3e^{i(\pi /6 + 2\pi k / 3)}.\\
		\Rightarrow 3e^{i\pi/6} &=  \frac{3 \sqrt{3}}{2}+\frac{3 i}{2}. \\
		3 e^{5 i \pi /6} &= -\frac{3 \sqrt{3}}{2}+\frac{3 i}{2}. && \footnotemark \\
		3 e^{3 \pi  i / 2} &= -3i.
	\end{align*}
	\footnotetext{Don't settle for the just the first root discovered! Keep plugging in integers for $k$ until values begin to repeat.}

	\begin{align*}
		\sqrt[4]{-16} &= \sqrt[4]{16e^{i(\pi + 2\pi k)}} \; \forall \; k \in \Z.\\
		&= 2e^{i(\pi / 4 + \pi k / 2)}.\\
		\Rightarrow 2 e^{i \pi / 4} &= (1+i) \sqrt{2}.\\
		2 e^{3 i \pi / 4} &= (-1+i) \sqrt{2}.\\
		2 e^{-3 i \pi / 4} &= (-1-i) \sqrt{2}.\\
		2 e^{-i \pi / 4} &= (1-i) \sqrt{2}.
	\end{align*}

	\section{Problem 5.}
	\begin{description}
		\item \textit{Use the quadratic formula to solve $x^2 - (5-i)x + (8-i) = 0.$}
	\end{description}

	\begin{align*}
		\frac{5-i \pm \sqrt{(5-i)^2+4(8-i)}}{2} &= \frac{5}{2}-\frac{i}{2} \pm \frac{1}{2}-\frac{3 i}{2}.
	\end{align*}

	$x = 3 - 2i$ or $x = 2 + i$.

	\newpage

	\section{Problem 6.}
	\begin{description}
		\item \textit{Compute the eighth roots of unity. Plot them on a graph and connect these points. What shape do you obtain?}
	\end{description}

	\begin{align*}
		\sqrt[8]{1} &= \sqrt[8]{e^{i(0 + 2\pi k)}} \; \forall \; k \in \Z.\\
		&= e^{i\pi k / 4}.
	\end{align*}

	For $k \in \{0,1,\ldots,7\}$,

	\begin{marginfigure}
		\centering
		\includegraphics[height=5cm]{pr6.pdf}
		\caption{We appear to have something resembling the unit circle.}
	\end{marginfigure}

	\begin{align*}
		e^{0} &= 1.\\
		e^{i \pi /4} &= \frac{1+i}{\sqrt{2}}.\\
		i &= i.\\
		e^{3 i \pi / 4} &= -\frac{1-i}{\sqrt{2}}.\\
		-1 &= -1.\\
		e^{-3 i \pi / 4} &= -\frac{1+i}{\sqrt{2}}.\\
		-i &= -i.\\
		e^{-i \pi / 4} &= \frac{1-i}{\sqrt{2}}.
	\end{align*}

	\section{Problem 7.}
	\begin{description}
		\item \textit{By using Euler's identity, show that the conjugate of $e^{i\theta}$ equals $e^{-i\theta}$. \footnote{Euler's identity: $e^{i\theta} = \cos\theta + i\sin\theta.$ Often the first question on midterms given by Dr. Sittinger.}}
	\end{description}

	\begin{align*}
		\overline{e^{i\theta}} &= \overline{\cos\theta + i \sin\theta}.\\
		&= \cos\theta - i \sin\theta. && \footnotemark\\
		&= \cos(-\theta) + i\sin(-\theta). && \footnotemark \\
		&= e^{-i\theta}.
	\end{align*}
	\footnotetext[2]{$\overline{a+b} = a-b.$}
	\footnotetext{$\cos$ is an even function, so $\cos\theta = \cos(-\theta).$ Similarly, $\sin$ is odd, so $-\sin\theta = \sin(-\theta).$}


